{
  "label" : "lock",
  "action" : "sleep .5 && $HOME/bin/lock",
  "text" : "Lock (l)",
  "keybind" : "l"
}
{
  "label" : "logout",
  "action": "loginctl terminate-session $(loginctl session-status | head -n 1 | awk '{print $1}')"
  "text" : "Logout (e)",
  "keybind" : "e"
}
{
  "label" : "shutdown",
  "action" : "systemctl poweroff",
  "text" : "Shutdown (s)",
  "keybind" : "s"
}
{
  "label" : "suspend",
  "action" : "systemctl suspend",
  "text" : "Suspend (u)",
  "keybind" : "u"
}
{
  "label" : "reboot",
  "action" : "systemctl reboot",
  "text" : "Reboot (r)",
  "keybind" : "r"
}

